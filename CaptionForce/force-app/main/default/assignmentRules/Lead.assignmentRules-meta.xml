<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Industry-Based Assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Education_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Industry</field>
                <operation>equals</operation>
                <value>Education</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Entertainment_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Industry</field>
                <operation>equals</operation>
                <value>Entertainment</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Misc_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.Industry</field>
                <operation>notEqual</operation>
                <value>Education,Entertainment</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
