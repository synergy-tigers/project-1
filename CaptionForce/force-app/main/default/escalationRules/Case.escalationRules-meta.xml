    <escalationRule>
        <fullName>Captioning</fullName>
        <active>true</active>
        <ruleEntry>
            <businessHoursSource>Case</businessHoursSource>
            <criteriaItems>
                <field>Account.SLA__c</field>
                <operation>equals</operation>
                <value>Gold,Platinum</value>
            </criteriaItems>
            <escalationAction>
                <assignedTo>VIP_Cases</assignedTo>
                <assignedToTemplate>unfiled$public/SupportEscalatedCaseNotification</assignedToTemplate>
                <assignedToType>Queue</assignedToType>
                <minutesToEscalation>30</minutesToEscalation>
                <notifyCaseOwner>false</notifyCaseOwner>
            </escalationAction>
            <escalationStartTime>CaseCreation</escalationStartTime>
        </ruleEntry>
